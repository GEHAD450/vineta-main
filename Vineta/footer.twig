<footer section-id="{{ sectionId }}" id="footer" class="footer-default xl-pb-70" data-lg-scroll-offset="130" data-md-scroll-offset="130" data-sm-scroll-offset="94">
    <!-- Footer Top -->
    <div class="footer-top">
        <div class="{% if settings.full_width %} container-fluid {% else %} container custom-container {% endif %}">
            <div class="footer-top-wrap">
                <!-- Footer Logo -->
                <div class="footer-logo">
                    {% if settings.business_location_image %}
                        <a href="{{ store.url }}">
                            <img src="{{ settings.business_location_image }}" class="logo" alt="{{ store.name }}" loading="lazy">
                        </a>
                    {% else %}
                        <a href="{{ store.url }}">
                            <img src="{{ store.logo }}" class="logo" alt="{{ store.name }}" loading="lazy">
                        </a>
                    {% endif %}
                </div>
                
                <!-- Social Media Icons -->
                {% if not (settings.social_media_hide) %}
                <ul class="tf-social-icon style-large">
                    {% if settings.social_media_facebook or store.social_media.items.facebook %}
                    <li>
                        <a href="{% if settings.social_media_facebook %}{{ settings.social_media_facebook }}{% else %}https://www.facebook.com/{{ store.social_media.items.facebook }}{% endif %}" class="social-item social-facebook" target="_blank">
                            <i class="fa fa-facebook"></i>
                        </a>
                    </li>
                    {% endif %}
                    
                    {% if settings.social_media_instagram or store.social_media.items.instagram %}
                    <li>
                        <a href="{% if settings.social_media_instagram %}{{ settings.social_media_instagram }}{% else %}https://www.instagram.com/{{ store.social_media.items.instagram }}{% endif %}" class="social-item social-instagram" target="_blank">
                            <i class="fa fa-instagram"></i>
                        </a>
                    </li>
                    {% endif %}
                    
                    {% if settings.social_media_twitter or store.social_media.items.twitter %}
                    <li>
                        <a href="{% if settings.social_media_twitter %}{{ settings.social_media_twitter }}{% else %}https://x.com/{{ store.social_media.items.twitter }}{% endif %}" class="social-item social-x" target="_blank">
                            <i class="fa fa-twitter"></i>
                        </a>
                    </li>
                    {% endif %}
                    
                    {% if settings.social_media_snapchat or store.social_media.items.snapchat %}
                    <li>
                        <a href="{% if settings.social_media_snapchat %}{{ settings.social_media_snapchat }}{% else %}https://www.snapchat.com/add/{{ store.social_media.items.snapchat }}{% endif %}" class="social-item social-linkedin" target="_blank">
                            <i class="fa fa-snapchat-ghost"></i>
                        </a>
                    </li>
                    {% endif %}
                    
                    {% if settings.social_media_tiktok or store.social_media.items.tiktok %}
                    <li>
                        <a href="{% if settings.social_media_tiktok %}{{ settings.social_media_tiktok }}{% else %}https://www.tiktok.com/@{{ store.social_media.items.tiktok }}{% endif %}" class="social-item social-tiktok" target="_blank">
                            <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" viewBox="0 0 24 24" xml:space="preserve"><path d="M19.589 6.686a4.793 4.793 0 0 1-3.77-4.245V2h-3.445v13.672a2.896 2.896 0 0 1-5.201 1.743l-.002-.001.002.001a2.895 2.895 0 0 1 3.183-4.51v-3.5a6.329 6.329 0 0 0-5.394 10.692 6.33 6.33 0 0 0 10.857-4.424V8.687a8.182 8.182 0 0 0 4.773 1.526V6.79a4.831 4.831 0 0 1-1.003-.104z"/></svg>
                        </a>
                    </li>
                    {% endif %}
                    
                    {% if settings.social_media_website or store.social_media.items.website %}
                    <li>
                        <a href="{% if settings.social_media_website %}{{ settings.social_media_website }}{% else %}{{ store.social_media.items.website }}{% endif %}" class="social-item social-website" target="_blank">
                            <i class="fa fa-globe"></i>
                        </a>
                    </li>
                    {% endif %}
                </ul>
                {% endif %}
            </div>
        </div>
    </div>
    
    <!-- Footer Body -->
    <div class="footer-body">
        <div class="{% if settings.full_width %} container-fluid {% else %} container custom-container {% endif %}">
            <div class="row-footer">
                <!-- Business Contact Section -->
                {% if not (settings.business_location_hide) %}
                <div class="footer-col-block s1">
                    <div class="footer-heading footer-heading-mobile text-xl fw-medium">
                        {% if locals.business_contact %}{{ locals.business_contact }}{% else %}Business Contact{% endif %}
                    </div>
                    <div class="tf-collapse-content">
                        <div class="footer-contact">
                            <ul class="footer-info">
                                {% if settings.business_location_address or store.business_location.show_location %}
                                <li class="item">
                                    <span class="box-icon">
                                        <svg width="12" height="14" viewBox="0 0 12 14" fill="none" xmlns="http://www.w3.org/2000/svg">
                                            <path d="M10.1869 1.73431C9.06854 0.61592 7.58155 0 5.99993 0C4.4183 0 2.93129 0.61592 1.81292 1.73431C0.694534 2.85273 0.0786133 4.33969 0.0786133 5.92129C0.0786133 9.12084 3.10388 11.7821 4.72917 13.2118C4.95504 13.4105 5.15008 13.582 5.30547 13.7272C5.50016 13.9091 5.75006 14 5.9999 14C6.24979 14 6.49964 13.9091 6.69435 13.7272C6.84975 13.582 7.04479 13.4105 7.27065 13.2118C8.89594 11.7821 11.9212 9.12084 11.9212 5.92129C11.9212 4.33969 11.3053 2.85273 10.1869 1.73431ZM6.72897 12.5961C6.49816 12.7991 6.29885 12.9744 6.13451 13.1279C6.05902 13.1984 5.94078 13.1984 5.86526 13.1279C5.70095 12.9744 5.50161 12.7991 5.2708 12.596C3.74283 11.2519 0.898656 8.75001 0.898656 5.92131C0.898656 3.1085 3.18704 0.820124 5.99987 0.820124C8.81268 0.820124 11.1011 3.1085 11.1011 5.92131C11.1011 8.75001 8.25694 11.2519 6.72897 12.5961Z" fill="#0D0D0D"/>
                                            <path d="M6.00008 3.08887C4.56122 3.08887 3.39062 4.25943 3.39062 5.69829C3.39062 7.13715 4.56122 8.30772 6.00008 8.30772C7.43894 8.30772 8.6095 7.13715 8.6095 5.69829C8.6095 4.25943 7.43894 3.08887 6.00008 3.08887ZM6.00008 7.48759C5.01343 7.48759 4.21072 6.68489 4.21072 5.69826C4.21072 4.71164 5.01343 3.90894 6.00008 3.90894C6.98673 3.90894 7.7894 4.71164 7.7894 5.69826C7.7894 6.68489 6.98673 7.48759 6.00008 7.48759Z" fill="#0D0D0D"/>
                                        </svg>
                                    </span>
                                    {% if store.business_location.lat %}
                                        <a target="_blank" href="https://maps.google.com/?q={{ store.business_location.lat }},{{ store.business_location.lng }}">
                                    {% endif %}
                                    {% if settings.business_location_address %}
                                        {{ settings.business_location_address }}
                                    {% else %}
                                        {{ store.business_location.country.name }}, {{ store.business_location.city.name }}, {{ store.business_location.district }}, {{ store.business_location.street }}
                                    {% endif %}
                                    {% if store.business_location.lat %}
                                        </a>
                                    {% endif %}
                                </li>
                                {% endif %}
                                
                                {% if settings.business_location_phone or store.social_media.items.phone %}
                                <li class="item">
                                    <span class="box-icon">
                                        <svg width="14" height="14" viewBox="0 0 14 14" fill="none" xmlns="http://www.w3.org/2000/svg">
                                            <path d="M11.6666 9.4091L10.9841 8.4641C10.7324 8.12417 10.3574 7.89649 9.93972 7.82996C9.522 7.76343 9.09484 7.86335 8.74998 8.10827L8.09081 8.59243C7.13399 7.73698 6.36526 6.69214 5.83331 5.5241L6.49248 5.04577C6.837 4.79478 7.06794 4.41741 7.13464 3.99641C7.20135 3.5754 7.09838 3.14513 6.84831 2.79993L6.18331 1.86077C5.93339 1.51663 5.55703 1.28582 5.13698 1.21909C4.71693 1.15236 4.28757 1.25518 3.94331 1.50493L2.91664 2.23993C2.58681 2.47798 2.33009 2.80349 2.17547 3.17972C2.02086 3.55595 1.9745 3.96792 2.04164 4.3691C2.33803 6.09644 3.02915 7.73206 4.06118 9.14856C5.0932 10.5651 6.43827 11.7242 7.99164 12.5358C8.30603 12.696 8.65376 12.78 9.00664 12.7808C9.46814 12.7804 9.91756 12.6333 10.29 12.3608L11.3108 11.6666C11.4837 11.5428 11.6302 11.3858 11.7419 11.2048C11.8535 11.0238 11.9281 10.8224 11.9612 10.6123C11.9943 10.4023 11.9853 10.1877 11.9347 9.98113C11.8842 9.77457 11.793 9.58012 11.6666 9.4091Z" fill="#0D0D0D"/>
                                        </svg>
                                    </span>
                                    <a href="tel:{% if settings.business_location_phone %}{{ settings.business_location_phone }}{% else %}{{ store.social_media.items.phone }}{% endif %}">
                                        {% if settings.business_location_phone %}{{ settings.business_location_phone }}{% else %}{{ store.social_media.items.phone }}{% endif %}
                                    </a>
                                </li>
                                {% endif %}
                                
                                {% if settings.business_location_email %}
                                <li class="item">
                                    <span class="box-icon">
                                        <svg width="14" height="14" viewBox="0 0 14 14" fill="none" xmlns="http://www.w3.org/2000/svg">
                                            <path d="M11.8125 2.625H2.1875C1.8394 2.625 1.50556 2.76328 1.25942 3.00942C1.01328 3.25556 0.875 3.5894 0.875 3.9375V10.0625C0.875 10.4106 1.01328 10.7444 1.25942 10.9906C1.50556 11.2367 1.8394 11.375 2.1875 11.375H11.8125C12.1606 11.375 12.4944 11.2367 12.7406 10.9906C12.9867 10.7444 13.125 10.4106 13.125 10.0625V3.9375C13.125 3.5894 12.9867 3.25556 12.7406 3.00942C12.4944 2.76328 12.1606 2.625 11.8125 2.625Z" fill="#0D0D0D"/>
                                        </svg>
                                    </span>
                                    <a href="mailto:{{ settings.business_location_email }}">{{ settings.business_location_email }}</a>
                                </li>
                                {% endif %}
                            </ul>
                            
                            {% if store.business_location.lat %}
                            <a href="https://maps.google.com/?q={{ store.business_location.lat }},{{ store.business_location.lng }}" class="tf-btn btn-line-dark fw-normal" target="_blank">
                                <span class="text-sm text-transform-none">{% if locals.get_direction %}{{ locals.get_direction }}{% else %}Get Direction{% endif %}</span>
                                <i class="fa fa-external-link fs-8"></i>
                            </a>
                            {% endif %}
                        </div>
                    </div>
                </div>
                {% endif %}

                <!-- Newsletter Section -->
                <div class="footer-inner-wrap footer-col-block s2">
                    <div class="footer-heading footer-heading-mobile text-xl fw-medium">
                        {% if locals.subscribe_newsletter %}{{ locals.subscribe_newsletter }}{% else %}Subscribe Newsletter{% endif %}
                    </div>
                    <div class="tf-collapse-content">
                        <div class="footer-newsletter">
                            <p>{% if locals.newsletter_description %}{{ locals.newsletter_description }}{% else %}We invite you to register to read the latest news, offers and events about our company. We promise not spam your inbox.{% endif %}</p>
                            <form class="form-newsletter" method="POST">
                                <div class="form__entry entry_block">
                                    <fieldset class="entry__field">
                                        <input class="input radius-60" type="email" name="email" autocomplete="off" placeholder="{% if locals.enter_email %}{{ locals.enter_email }}{% else %}Enter your e-mail...{% endif %}" required />
                                    </fieldset>
                                </div>
                                <div class="button-submit">
                                    <button class="sib-form-block__button subscribe-button radius-60" type="submit">
                                        <svg width="18" height="18" viewBox="0 0 18 18" fill="none" xmlns="http://www.w3.org/2000/svg">
                                            <g clip-path="url(#clip0_5296_3345)">
                                                <path d="M17.7938 8.50229L17.7931 8.50162L14.1192 4.84537C13.8439 4.57147 13.3988 4.57249 13.1248 4.84776C12.8508 5.123 12.8519 5.56818 13.1271 5.84212L15.5938 8.29687H0.703125C0.314789 8.29687 0 8.61166 0 9C0 9.38833 0.314789 9.70312 0.703125 9.70312H15.5938L13.1272 12.1579C12.8519 12.4318 12.8509 12.877 13.1248 13.1522C13.3988 13.4275 13.844 13.4285 14.1192 13.1546L17.7932 9.49837L17.7938 9.4977C18.0692 9.22285 18.0683 8.77623 17.7938 8.50229Z" fill="white"></path>
                                            </g>
                                        </svg>
                                    </button>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
                
                <!-- Links Sections -->
                <div class="footer-inner-wrap s3">
                    {% if not (settings.links_1_hide) %}
                    <div class="footer-col-block inner-col">
                        <div class="footer-heading footer-heading-mobile text-xl fw-medium">
                            {% if settings.links_1_title %}{{ settings.links_1_title }}{% else %}About Us{% endif %}
                        </div>
                        <div class="tf-collapse-content">
                            <ul class="footer-menu-list">
                                {% for link in settings.links_1_links %}
                                    {% if link.title %}
                                    <li>
                                        <a href="{{ link.url }}">{{ link.title }}</a>
                                    </li>
                                    {% endif %}
                                {% endfor %}
                            </ul>
                        </div>
                    </div>
                    {% endif %}
                    
                    {% if not (settings.links_2_hide) %}
                    <div class="footer-col-block inner-col">
                        <div class="footer-heading footer-heading-mobile text-xl fw-medium">
                            {% if settings.links_2_title %}{{ settings.links_2_title }}{% else %}Resource{% endif %}
                        </div>
                        <div class="tf-collapse-content">
                            <ul class="footer-menu-list">
                                {% for link in settings.links_2_links %}
                                    {% if link.title %}
                                    <li>
                                        <a href="{{ link.url }}">{{ link.title }}</a>
                                    </li>
                                    {% endif %}
                                {% endfor %}
                            </ul>
                        </div>
                    </div>
                    {% endif %}
                    
                    {% if not (settings.links_3_hide) %}
                    <div class="footer-col-block inner-col">
                        <div class="footer-heading footer-heading-mobile text-xl fw-medium">
                            {% if settings.links_3_title %}{{ settings.links_3_title }}{% else %}Support{% endif %}
                        </div>
                        <div class="tf-collapse-content">
                            <ul class="footer-menu-list">
                                {% for link in settings.links_3_links %}
                                    {% if link.title %}
                                    <li>
                                        <a href="{{ link.url }}">{{ link.title }}</a>
                                    </li>
                                    {% endif %}
                                {% endfor %}
                            </ul>
                        </div>
                    </div>
                    {% endif %}
                </div>
            </div>
        </div>
    </div>
    
    <!-- Footer Bottom -->
    <div class="footer-bottom">
        <div class="{% if settings.full_width %} container-fluid {% else %} container custom-container {% endif %}">
            <div class="footer-bottom-wrap">
                <!-- Copyright -->
                <p class="text-dark">
                    <a target="_blank" href="{% if settings.global_settings_copyrights_link %}{{ settings.global_settings_copyrights_link }}{% else %}https://mt-themes.com{% endif %}">
                        {{ store.copyrights | raw }}
                    </a>
                </p>
                
                <!-- Payment Methods & Apps -->
                <div class="footer-bottom-right">
                    <!-- Mobile Apps -->
                    {% if store.apps.ios_app_id or store.apps.android_package_name or settings.global_settings_ios_id or settings.global_settings_android_id %}
                    <div class="footer-apps d-flex align-items-center">
                        {% if store.apps.ios_app_id or settings.global_settings_ios_id %}
                        <a href="https://apps.apple.com/app/id={% if store.apps.ios_app_id %}{{ store.apps.ios_app_id }}{% else %}{{ settings.global_settings_ios_id }}{% endif %}" target="_blank">
                            <img src="{{ asset_url ~ 'app-store.svg' }}" loading="lazy" alt="App Store" width="115" height="35">
                        </a>
                        {% endif %}
                        {% if store.apps.android_package_name or settings.global_settings_android_id %}
                        <a href="https://play.google.com/store/apps/details?id={% if store.apps.android_package_name %}{{ store.apps.android_package_name }}{% else %}{{ settings.global_settings_android_id }}{% endif %}" target="_blank">
                            <img src="{{ asset_url ~ 'google-play.svg' }}" loading="lazy" alt="Google Play" width="115" height="35">
                        </a>
                        {% endif %}
                    </div>
                    {% endif %}
                    
                    <!-- Payment Methods -->
                    {% if store.shipping_methods | length > 0 or store.payment_methods | length > 0 %}
                    <ul class="tf-payment">
                        {% if not settings.companies_vat_hide_companies %}
                            {% for shipping_method in store.shipping_methods %}
                            <li class="item">
                                <img src="{{ imageUrl(shipping_method.icon,{ h: 80, q: 100 }) }}" loading="lazy" alt="{{ shipping_method.name }}">
                            </li>
                            {% endfor %}
                        {% endif %}
                        {% if not settings.companies_vat_hide_payment_methods %}
                            {% for payment_method in store.payment_methods %}
                            <li class="item">
                                <img src="{{ imageUrl(payment_method.icon,{ h: 80, q: 100 }) }}" loading="lazy" alt="{{ payment_method.name }}">
                            </li>
                            {% endfor %}
                        {% endif %}
                    </ul>
                    {% endif %}
                </div>
            </div>
        </div>
    </div>

        <!-- VAT Information (Hidden in new design) -->
        {% if false and not (settings.companies_vat_hide) %}
        <div class="col-lg-auto links vatSection">
          <div class="vat-crn-sections d-flex flex-row flex-wrap" style="color:var(--color-vat)">
            {% if not settings.companies_vat_hide_business_platform %}
                {% if store.social_media.items.business_center or settings.companies_vat_business_platform_number %}
